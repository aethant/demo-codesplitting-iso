{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "plugins": [],
  "rules": {
    "no-console": 1,
    "no-multi-str": 0,
    "space-in-parens": [2, "always"],
    "computed-property-spacing": [2, "always"],
    "array-bracket-spacing": [2, "always"],
    "arrow-body-style": 0,
    "comma-dangle": [2, "always"],
    "indent": [1, 2],
    "no-confusing-arrow": 0,
    "semi": [ 2, "never" ],
    "jsx-a11y/href-no-hash": 0,
    "no-debugger": 0
  },
  "globals": {
    "document": true,
    "window": true
  },
  "env": {
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": false
    }
  }
}